<p id="notice"><%= notice %></p>

<h1>Evaluations</h1>

<table>
  <thead>
    <tr>
      <th>Rated</th>
      <th>Rater</th>
      <th>Helpful</th>
      <th>Focused</th>
      <th>Responsible</th>
      <th>Creative</th>
      <th>Leadership</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @evaluations.each do |evaluation| %>
      <tr>
        <td><%= evaluation.rated_id %></td>
        <td><%= evaluation.rater_id %></td>
        <td><%= evaluation.helpful %></td>
        <td><%= evaluation.focused %></td>
        <td><%= evaluation.responsible %></td>
        <td><%= evaluation.creative %></td>
        <td><%= evaluation.leadership %></td>
        <td><%= link_to 'Show', evaluation %></td>
        <td><%= link_to 'Edit', edit_evaluation_path(evaluation) %></td>
        <td><%= link_to 'Destroy', evaluation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Evaluation', new_evaluation_path %>
